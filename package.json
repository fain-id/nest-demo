{
  "name": "Nestjs-starter",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "dev:client": "cross-env NODE_ENV=development node config/dev-server.js",
    "dev:server": "nodemon --config config/nodemon.json",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:server": "tsc -p tsconfig-server.json",
    "build": "rimraf build &&  npm run build:server && npm run build:client",
    "production": "cross-env NODE_ENV=production pm2 start pm2.config.json"
  },
  "keywords": [
    "angular",
    "typescript",
    "ts",
    "webpack",
    "es6"
  ],
  "author": "tanbo",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@nestjs/common": "^5.3.7",
    "@nestjs/core": "^5.3.7",
    "axios": "^0.18.0",
    "core-js": "^2.5.1",
    "normalize.css": "7.0.0",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.0",
    "@angularclass/hmr": "2.1.3",
    "@nestjs/testing": "^5.3.7",
    "@ngtools/webpack": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "2.8.2",
    "@types/node": "8.0.53",
    "@types/webvr-api": "0.0.35",
    "angular-hot-reload-loader": "1.0.2",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "7.1.6",
    "awesome-typescript-loader": "5.0.0",
    "codelyzer": "~4.2.1",
